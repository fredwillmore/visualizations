<p id="notice"><%= notice %></p>

<h1>Listing Business Entities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_entities.each do |business_entity| %>
      <tr>
        <td><%= link_to 'Show', business_entity %></td>
        <td><%= link_to 'Edit', edit_business_entity_path(business_entity) %></td>
        <td><%= link_to 'Destroy', business_entity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @business_entities %>

<br>

<%= link_to 'New Business entity', new_business_entity_path %>

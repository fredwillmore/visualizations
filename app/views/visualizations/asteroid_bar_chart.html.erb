<h2>Asteroid Count By Condition Code - Linear Scale</h2>
<%= react_component 'BarChart',
      yScale: 'linear',
      dataUrl: '/visualizations/asteroid_bar_chart.json',
      labelField: 'condition_code',
      valueField: 'count',
      xAxisLabelText: 'Condition Code',
      yAxisLabelText: 'Asteroid Count',
      yAxisLabelOffset: 60 %>

<h2>Asteroid Count By Condition Code - Logarithmic Scale</h2>
<%= react_component 'BarChart',
      yScale: 'log',
      dataUrl: '/visualizations/asteroid_bar_chart.json',
      labelField: 'condition_code',
      valueField: 'count',
      xAxisLabelText: 'Condition Code',
      yAxisLabelText: 'Asteroid Count' %>


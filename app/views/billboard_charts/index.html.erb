<p id="notice"><%= notice %></p>

<h1>Listing Billboard Charts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billboard_charts.each do |billboard_chart| %>
      <tr>
        <td><%= billboard_chart.chart_date %></td>
        <td><%= link_to 'Show', billboard_chart %></td>
        <td><%= link_to 'Edit', edit_billboard_chart_path(billboard_chart) %></td>
        <td><%= link_to 'Destroy', billboard_chart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Billboard chart', new_billboard_chart_path %>
